<Window x:Class="DeskTop.FrmDaylyStat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeskTop"
        mc:Ignorable="d"
        Title="Ежедневная статистика" Height="600" Width="600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <DataGrid Name="dgStat" AutoGenerateColumns="False" ItemsSource="{Binding}"/>
        <Button Name="btnOk" IsDefault="True" Grid.Row="1" Width="80" Height="25" Content="OK" Click="btnOk_Click"/>
    </Grid>
</Window>
